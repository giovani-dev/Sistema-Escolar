@startuml


interface DataToClass {
    # info: DataClass

    + equals(other: Any?): Boolean
}

class InfoAluno <<data>> {
    + nome: String
    + sobrenome: String
    + codigo: Int
}

class Aluno {
    info: InfoAluno

    + equals(other: Any?): Boolean
}

DataToClass *-- Aluno
InfoAluno *-- Aluno


class InfoCurso <<data>> {
    + nome: String
    + sobrenome: String
    + codigo: Int
}

class Curso {
    info: InfoAluno

    + equals(other: Any?): Boolean
}

DataToClass *-- Curso
InfoCurso *-- Curso

@enduml